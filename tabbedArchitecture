                    travelDaysTable.getColumnModel().getColumn(1).setCellRenderer(new DefaultTableCellRenderer() {

                        @Override
                        public Component getTableCellRendererComponent(JTable table, Object value, boolean isSelected, boolean hasFocus, int row, int column) {

                            DefaultTableCellRenderer comp = (DefaultTableCellRenderer) super.getTableCellRendererComponent(table, value, isSelected, hasFocus, row, column);

                            int test = 0;
                            Boolean finished = false;
                            if (table.getModel().getValueAt(row, column).equals(dateIncDepart)) {
                                comp.setForeground(Color.blue);
                            } else if (table.getModel().getValueAt(row, column).equals(dateIncArrive)) {
                                comp.setForeground(Color.orange);
                            } else {
                                comp.setForeground(Color.black);
                            }

                            return comp;
                        }
