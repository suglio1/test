        int LO2Min_depart = Integer.parseInt(LO2_Dmin.toString());
        System.out.println(LO2Min_depart);
        
        SUBMITBUTTON.setEnabled(false);
        CLEARBUTTON.setEnabled(true);
        
        String entryCurrent = currentCity.getText().trim();
        String entryDestination = destinationCity.getText().trim();

        String layOver1String = layoverCity1.getText().trim();
        String layOver2String = layoverCity2.getText().trim();

        int flag1 = 0;
        int flag2 = 0;
        int flag3 = 0;
        int flag4 = 0;

        Boolean currentEmpty = false;
        Boolean destinationEmpty = false;

        if (currentCity.getText() == null) {
            System.out.println("Must enter a current city");
            currentEmpty = true;

        } else {
            if (hashTZ.containsKey(entryCurrent)) {

                System.out.println("True, exists here");

                Object value = hashTZ.get(entryCurrent);
                int inum = (Integer) value;

                System.out.println(inum);

                flag1 = 1;

            } else {
                System.out.println("False. Not found in data collection");
            }
        }

        if (destinationCity.getText() == null) {
            System.out.println("Must enter a destination city");
            destinationEmpty = true;
        } else {
            if (hashTZ.containsKey(entryDestination)) {
                System.out.println("True, exists here");

                Object value = hashTZ.get(entryDestination);
                int inum = (Integer) value;
                System.out.println(inum);

                flag2 = 1;

            } else {
                System.out.println("False. Not found in data collection");
            }
        }

        if (hashTZ.containsKey(entryCurrent) == false || hashTZ.containsKey(entryDestination) == false) {
            JOptionPane.showMessageDialog(null, "City doesn't exist");
            SUBMITBUTTON.setEnabled(true);
            CLEARBUTTON.setEnabled(false);
        }

        if (layoverCity1.getText().equals("")) {
            System.out.println("No Layover picked");
        } else {
            if (hashTZ.containsKey(layOver1String)) {

                System.out.println("True, exists here");

                Object value = hashTZ.get(layOver1String);
                int inum = (Integer) value;
                System.out.println(inum);

                flag3 = 1;

            } else {
                System.out.println("False. Not found in data collection");
            }
        }
        
