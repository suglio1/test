    private void CLEARBUTTONActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        
        alexTable.setModel(new DefaultTableModel());

        DefaultTableModel dm = (DefaultTableModel) alexTable.getModel();

        for (int i = dm.getRowCount() - 1; i >= 0; i--) {
            dm.removeRow(i);
        }

        currentCity.setText(null);
        destinationCity.setText(null);
        layoverCity1.setText(null);
        layoverCity2.setText(null);

        DefaultTableModel tbm = (DefaultTableModel) timeZoneTable.getModel();

        for (int i = tbm.getRowCount() - 1; i >= 0; i--) {
            tbm.removeRow(i);
        }

        DefaultTableModel tbm3 = (DefaultTableModel) travelDaysTable.getModel();

        for (int i = tbm3.getRowCount() - 1; i >= 0; i--) {
            tbm3.removeRow(i);
        }

        shiftBegin.setCalendar(null);
        shiftEnd.setCalendar(null);

        departureDate.setCalendar(null);
        arrivalDate.setCalendar(null);

//        bedTimeH.setSelectedIndex(0);
//        bedTimeM.setSelectedIndex(0);
//
//        wakeUpH.setSelectedIndex(0);
//        wakeUpM.setSelectedIndex(0);

        ////////////
