    private void SUBMITBUTTONActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        
        System.out.println(LO1_arrivalDate.getDate());
        System.out.println(LO1_departDate.getDate());
        System.out.println(LO2_arrivalDate.getDate());
        System.out.println(LO2_departDate.getDate());

       
        
        Object LO1_Ahr = LO1_arrivalHour.getSelectedItem();
        int LO1Hour_arrive = Integer.parseInt(LO1_Ahr.toString());
        System.out.println(LO1Hour_arrive);

